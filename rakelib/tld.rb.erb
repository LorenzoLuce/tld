class TLD
  class <%= klass %> < TLD
    def type
      :<%= type %>
    end
    <% tlds.each do |tld| -%>
    class TLD<%= tld[:tld].split(/(\W)/).map(&:capitalize).join %> < <%= klass %> #:nodoc:
      self.tld = %q{<%= tld[:tld].downcase %>}
      self.sponsoring_organization = %q{<%= tld[:sponsoring_organization] %>}
    end
    <% end -%>
  end # end <%= klass %>
end # end TLD
